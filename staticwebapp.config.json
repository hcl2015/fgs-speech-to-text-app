{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "mimeTypes": {
    ".js": "text/javascript"
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"],
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "HEAD", "OPTIONS"]
    }
  ],
  "platform": {
    "apiRuntime": "node:18"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
